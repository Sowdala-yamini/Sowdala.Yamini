#include <stdio.h>

int main()
{
    int n;
    printf("Enter the number of elements in the array:");
    scanf("%d",&n);
    int a[n];
    for(int i=0;i<n;i++)
    {
        printf("Enter the element %d in the array:",i+1);
        scanf("%d",&a[i]);
    }
    int k;
    printf("Enter the element whose frequency is to be found:");
    scanf("%d",&k);
    int sum=0;
    for(int i=0;i<n;i++)
    {
        if(a[i]==k)
        sum++;
    }
    printf("%d times",sum);

    return 0;
}
